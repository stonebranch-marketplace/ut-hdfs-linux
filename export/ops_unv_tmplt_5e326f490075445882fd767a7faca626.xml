###################################################
#
#    Name: ut_hdfs_file_monitor.py
#
#    Purpose: monitors a file on the Hadoop HDFS file System
#             and launches a Task if provided
#
#    Prerequisites:
#     - Universal Agent for Windows/Linux
#     - Python 2.7.x
#     - Requires the Python modules to be installed
#     - pip install hdfs
#
#     Version History:
#     1.0  Nils Buer    2018-10-12    initial Version
#     1.1  Nils Buer    2018-10-14    task exceution added
#     1.2  Nils Buer    2018-10-15    logging added
#          
####################################################
version = "1.2"

# Used modules

import datetime,time
import logging
import os,sys
import re
import subprocess
from hdfs import InsecureClient

# Sample CLI Command to test in Command Shell or dos Box

# -- logging settings INFO, DEBUG, WARNING, ERROR, CRITICAL

logging.basicConfig(level=logging.${ops_hdfs_level}, format=' %(asctime)s - %(levelname)s - %(message)s')

def main():
  script_setup()
  get_current_time()
  test_dir("${ops_hdfs_opsclidir}")                # test is cli dir is available
  monitor_file_on_hdfs()

# -- Set Variables from Universal Task Form or Command Line Input

def script_setup():
  logging.info("HDFS Filemonitor UT Version:" + version)
  logging.info("UT started with loglevel: ${ops_hdfs_level} ")
  logging.info("OPS CLI directory: ${ops_hdfs_opsclidir} ")
  logging.info("OMS User: ${_credentialUser("${ops_hdfs_omscred}")} ")
  logging.info("OMS Password: ${_credentialPwd("${ops_hdfs_omscred}")} ")
  logging.info("OMS Host: ${ops_hdfs_omshost}")
  logging.info("OMS PORT: ${ops_hdfs_omsport} ")
  logging.info("HDFS User: ${ops_hdfs_hdfsuser} ")
  logging.info("HDFS Port: ${ops_hdfs_hdfsport} ")
  logging.info("HDFS Port: ${ops_hdfs_hdfsurl} ")
  logging.info("File to monitor: ${ops_hdfs_file} ")
  logging.info("Task to launch: ${ops_hdfs_autorestart} ")
  logging.info("Trigger on exist: ${ops_hdfs_triggeronexits} ")
  logging.info("Task to launch: ${ops_hdfs_tasktolaunch} ")
  logging.info("Poll Interval: ${ops_hdfs_interval} ")
  logging.debug("Task monitor status variable initial: ${initial} ")
  

# --
# ## --&gt; Functions Go Here

def get_current_time():
    global current_time
    current_time = int(time.time() * 1000)

def test_dir(testdir):
    if os.path.isdir(testdir):
       logging.debug('Directory found ' + testdir)
    else:
       logging.error('Directory not found: ' + testdir)
       sys.exit(1) 


def check_task_status():
# checks if the Universal Task is running and has not been forced finished
  logging.debug("In check_task_status")
  process = subprocess.Popen(['/opt/universal/opscli/bin/ops-task-status','-f Yes','-o 300','-r OMS','-m ${ops_hdfs_omsport}@${ops_hdfs_omshost}','-u ${_credentialUser("${ops_hdfs_omscred}")}','-p ${_credentialPwd("${ops_hdfs_omscred}")}','task-instance-name=${ops_task_name}', 'task-status=Queued,Defined,Time_Wait,Started,Running,Resource_Wait,Resource_Requested,Started task-type=universal'], stdout=subprocess.PIPE)  
  status = process.communicate() 
  logging.debug("status:" + str(status))
  if re.search("No execs found matching",str(status)):
     return False
  else :
     return True


def check_file_on_hdfs():
  logging.debug("In check_file_on_hdfs")
  client = InsecureClient('${ops_hdfs_hdfsurl}:${ops_hdfs_hdfsport}',user='${ops_hdfs_hdfsuser}')
  # Listing all files inside a directory.
  data = client.status("${ops_hdfs_file}" , strict=False)
  logging.debug("Scanning for file:" +str(data))
  if str(data) != "None" :
    modificationTime = data.get('modificationTime')
    logging.debug("modificationTime:" +str(modificationTime))
    logging.debug("current Time    :" + str(current_time))
    if modificationTime &lt; current_time:
      logging.debug("file was created before start of monitor")
      if "${ops_hdfs_triggeronexits}" == "Yes" and ${initial} == True:
        logging.debug("Success file:${ops_hdfs_file} was created before start of monitor but triggeronexits was set")
        return True
    else:
      logging.debug("Success, file:${ops_hdfs_file} was created after start of monitor")
      return True
  else:
    logging.debug("file: ${ops_hdfs_file} was not found")
    return False

def restart_task():
  cmd = """${ops_hdfs_opsclidir}ops-task-launch -f Yes -o 300 -r OMS -m ${ops_hdfs_omsport}@${ops_hdfs_omshost} -u ${_credentialUser("${ops_hdfs_omscred}")} -p ${_credentialPwd("${ops_hdfs_omscred}")} task-name="${ops_task_name}" task-variables={initial="False"}"""
  # os.system(cmd)
  try:
        subprocess.check_output(cmd, shell=True)
  except subprocess.CalledProcessError as e:
        logging.error("##ERROR# Error Code: " + str(e.returncode))
        exit(1)
  logging.info("Task ${ops_task_name} re-started due to autorestart set to Yes")

def launch_task():
  cmd = """${ops_hdfs_opsclidir}ops-task-launch -f Yes -o 300 -r OMS -m ${ops_hdfs_omsport}@${ops_hdfs_omshost} -u ${_credentialUser("${ops_hdfs_omscred}")} -p ${_credentialPwd("${ops_hdfs_omscred}")} task-name="${ops_hdfs_tasktolaunch}" """
  # os.system(cmd)
  try:
        subprocess.check_output(cmd, shell=True)
  except subprocess.CalledProcessError as e:
        logging.error("##ERROR# Error Code: " + str(e.returncode))
        exit(1)
  logging.info("Task launched: ${ops_hdfs_tasktolaunch}")

def monitor_file_on_hdfs():
  #  check for file on hdfs as long as file is found
  found = False
  while not found:
    if check_task_status() :
      if check_file_on_hdfs() :
        found = True
        if "${ops_hdfs_tasktolaunch}" != "":
          launch_task()
        if "${ops_hdfs_autorestart}" == "Yes": 
          restart_task()
      else:
        found = False
        time.sleep(float(${ops_hdfs_interval}))
    else: 
        logging.error("${ops_task_name} is not started or running")
        exit(1) 
# -- Execute
